{
  "builds": [{
    "src": "myPortFolio/wsgi.py",
    "use": "@vercel/python",
    "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }
  }],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/myPortFolio/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "myPortFolio/wsgi.py"
    }
  ]
}